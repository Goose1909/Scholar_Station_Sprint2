<Page x:Class="SessionCreationWizardUPDATE.Page3"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:SessionCreationWizardUPDATE"
      mc:Ignorable="d" 
      d:DesignHeight="350" d:DesignWidth="525"
      Title="Page3">

    <Grid Background="#FFE9E9E9">
        <Label Content="Please select a class." HorizontalAlignment="Center" Margin="0,20,0,0" VerticalAlignment="Top" FontSize="20" FontFamily="Arial Rounded MT Bold" Foreground="DarkSlateGray" />
        <ComboBox x:Name="classContainer" HorizontalAlignment="Center" VerticalAlignment="Center" Width="120"/>
        <Label Content="Class:" HorizontalAlignment="Left" Margin="138,162,0,0" VerticalAlignment="Top" Foreground="#FF2F4F4D" FontFamily="Arial Rounded MT Bold"/>
        <Button Content="Back" HorizontalAlignment="Left" Margin="165,320,0,0" VerticalAlignment="Top" Width="75" Click="backClicked"/>
        <Button Content="Next" HorizontalAlignment="Left" Margin="277,320,0,0" VerticalAlignment="Top" Width="75" Click="nextClicked"/>
    </Grid>
</Page>
