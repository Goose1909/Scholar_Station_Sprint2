<Window x:Class="SessionCreationWizardUPDATE.WizardWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SessionCreationWizardUPDATE"
        mc:Ignorable="d"
        Title="WizardWindow" Height="350" Width="525">
    <Grid>
        <StackPanel Orientation="Horizontal" VerticalAlignment="Top" Height="35">
            <Button Content="Page1" MinWidth="100" Click="BtnClickWizardPage1"/>
            <Button Content="Page2" MinWidth="100" Margin="15,0,0,0" Click="BtnClickWizardPage2"/>
            <Button Content="Page3" MinWidth="100" Margin="15,0,0,0" Click="BtnClickWizardPage3"/>
        </StackPanel>
        <Frame x:Name="MainFrame" Margin="0,35,0,0" NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
