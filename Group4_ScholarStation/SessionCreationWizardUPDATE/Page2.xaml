<Page x:Class="SessionCreationWizardUPDATE.Page2"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:SessionCreationWizardUPDATE"
      mc:Ignorable="d" 
      d:DesignHeight="350" d:DesignWidth="525"
      Title="Page2">

    <Grid>
        <TextBlock Text="Date Selection" HorizontalAlignment="Center"/>
        <Label Content="Date:" VerticalAlignment="Center" Margin="50"/>
        <DatePicker x:Name="datePicker" VerticalAlignment="Center" HorizontalAlignment="Center" CalendarClosed="DatePickerDateChosen"/>
        <StackPanel Orientation="Horizontal" VerticalAlignment="Bottom" Height="35">
            <Button Content="Back" Margin="110,0,0,0" MaxHeight="20"/>
            <Button Content="Next" Margin="15,0,0,0" MaxHeight="20"/>
        </StackPanel>
    </Grid>
</Page>
