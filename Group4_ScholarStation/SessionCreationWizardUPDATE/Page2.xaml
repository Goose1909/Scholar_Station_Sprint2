<Page x:Class="SessionCreationWizardUPDATE.Page2"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:SessionCreationWizardUPDATE"
      mc:Ignorable="d" 
      d:DesignHeight="350" d:DesignWidth="525"
      Title="Page2" >

    <Grid Background="#FFE9E9E9">
        <TextBlock HorizontalAlignment="Center" FontSize="20" Margin="111,0,109,315" Foreground="DarkSlateGray" FontFamily="Arial Rounded MT Bold" Width="305"><Run Text="Please select a preferred date."/></TextBlock>
        <Label Content="Date:" VerticalAlignment="Center" Margin="155,161,145,163" Foreground="DarkSlateGray" FontFamily="Arial Rounded MT Bold"/>
        <DatePicker x:Name="datePicker" VerticalAlignment="Center" HorizontalAlignment="Center" CalendarClosed="DatePickerDateChosen" FontFamily="Arial Rounded MT Bold" Foreground="DarkSlateGray"/>
        <Button Content="Back" HorizontalAlignment="Left" Margin="171,320,0,0" VerticalAlignment="Top" Width="75" Foreground="DarkSlateGray" FontFamily="Arial Rounded MT Bold" Click="BackClicked"/>
        <Button Content="Next" HorizontalAlignment="Left" Margin="277,320,0,0" VerticalAlignment="Top" Width="75" Foreground="DarkSlateGray" FontFamily="Arial Rounded MT Bold" Click="NextClicked"/>
    </Grid>
</Page>
